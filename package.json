{
  "name": "cp1-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "bem-react-component-creator": "^1.2.0",
    "bootstrap": "^4.3.1",
    "enzyme": "^3.11.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.2",
    "react-scripts": "2.1.8",
    "react-table": "^7.7.0",
    "reactstrap": "8.9.0",
    "uniqid": "^5.3.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "enzyme-adapter-react-17-updated": "^1.0.2",
    "jest": "23.6.0",
    "prettier": "2.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --coverage --updateSnapshot",
    "prettier": "prettier --write .",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "setupFiles": [
      "<rootDir>/test_setup/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test_setup/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test_setup/fileMock.js"
    },
    "testPathIgnorePatterns": [
      "coverage",
      "node_modules",
      "test_setup",
      "assets"
    ],
    "globalSetup": "<rootDir>/test_setup/setup-env.js",
    "testMatch": [
      "<rootDir>/**/*.test.js"
    ],
    "verbose": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "coverageReporters": [
      "json",
      "lcovonly",
      "text",
      "clover"
    ]
  }
}
